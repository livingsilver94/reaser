{
	"name": "reaser",
	"version": "1.0.0",
	"description": "Remove web search URLs from your history",
	"scripts": {
		"prebuild": "tsc --outDir dist && node script/copy-manifest.js",
		"build": "web-ext build --overwrite-dest -s dist -a dist",
		"lint": "eslint --ignore-path .gitignore --ext=.mjs,.js,.json --fix .",
		"test": "jest --config jest.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/livingsilver94/reaser.git"
	},
	"keywords": [
		"firefox-webextension",
		"search",
		"history-management",
		"browser-extension"
	],
	"author": "Fabio Forni",
	"license": "GPL-3.0-or-later",
	"bugs": {
		"url": "https://github.com/livingsilver94/reaser/issues"
	},
	"homepage": "https://github.com/livingsilver94/reaser",
	"browserslist": [
		"Firefox ESR",
		"last 2 Chrome versions"
	],
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^3.1.0",
		"@typescript-eslint/parser": "^3.1.0",
		"eslint": "^6.8.0",
		"typescript": "^3.9.5",
		"web-ext": "^4.3.0",
		"webextension-polyfill-ts": "^0.16.0"
	},
	"dependencies": {}
}
