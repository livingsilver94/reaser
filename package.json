{
	"name": "reaser",
	"version": "1.0.0",
	"description": "Remove web search URLs from your history",
	"scripts": {
		"build": "parcel build --target browser --no-source-maps --no-cache --no-minify --out-dir dist reaser/manifest.json",
		"lint": "eslint --ignore-path .gitignore --ext=.mjs,.js,.json --fix .",
		"test": "jest --config jest.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/livingsilver94/reaser.git"
	},
	"keywords": [
		"firefox-webextension",
		"search",
		"history-management",
		"browser-extension"
	],
	"author": "Fabio Forni",
	"license": "GPL-3.0-or-later",
	"bugs": {
		"url": "https://github.com/livingsilver94/reaser/issues"
	},
	"homepage": "https://github.com/livingsilver94/reaser",
	"browserslist": [
		"Firefox ESR",
		"last 2 Chrome versions"
	],
	"devDependencies": {
		"@babel/core": "^7.6.2",
		"@babel/preset-env": "^7.6.2",
		"eslint": "^6.6.0",
		"eslint-config-standard": "^14.1.0",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-jest": "^23.0.3",
		"eslint-plugin-jsdoc": "^17.1.0",
		"eslint-plugin-json": "^1.4.0",
		"eslint-plugin-node": "^10.0.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-standard": "^4.0.1",
		"jest": "^24.9.0",
		"jest-webextension-mock": "^3.5.0",
		"parcel-bundler": "^1.12.4",
		"parcel-plugin-web-extension": "^1.5.2"
	},
	"dependencies": {}
}
